{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "hello.html",
      "default_icon": "icon.png"
    },
    "host_permissions": [
        "http://127.0.0.1:8000/"
    ],
    "permissions" : [
        "notifications",
        "background"    
    ],
    "background": {
        "service_worker": "background.js"
    }
  }